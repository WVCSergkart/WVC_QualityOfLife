<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<XmlExtensions.SettingsMenuDef>
		<defName>Settings_AnimaTree_WVC</defName>
		<modId>honkai.girls.sergkart</modId>
		<settings>
